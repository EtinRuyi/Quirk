@model Quirk.Models.ViewModels.LoginViewModel
@{
    ViewData["Title"] = "Login";
    Layout = "_LayoutNoNavbar";
}

<div class="container mx-auto">
    <div class="row justify-content-center">
        <div class="col-12 col-lg-6">
            <h1 class="mt-3 mb-3 h3">
                Welcome back!
            </h1>
        </div>
        <form method="post">
            <div class="mb-3">
                <label class="col-form-label">UserName</label>
                <input type="text" id="username" class="form-control" asp-for="UserName" required />
            </div>
            <div class="mb-3">
                <label class="col-form-label">Password</label>
                <input type="password" id="password" class="form-control" asp-for="Password" required minlength="6" />
            </div>

            <div class="mb-3">
                <button type="submit" class="btn btn-dark">Login</button>
                <a href="GoogleLogin" class="btn btn-primary">Login with Google</a>
            </div>
            <input type="hidden" asp-for="ReturnUrl" />
        </form>
        <p>Don't have an account?
            <a class="text-decoration-none"
               asp-area=""
               asp-controller="Auth"
               asp-action="Register">
                Sign Up
            </a>
        </p>
    </div>
</div>


